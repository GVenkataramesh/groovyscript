pipeline {
    agent any
    environment {
        PATH= "/opt/Maven/apache-maven-3.9.1/bin:$PATH"
    }

    stages {
        stage('gvrproject') {
            steps {
                echo 'Started gvrproject'
            }
        }
        stage('clone') {
            steps {
                git credentialsId: 'de1fb72f-a14a-48da-8e3a-6b8c3ab3427e', url: 'https://github.com/GVenkataramesh/groovyscript.git'
            }
        }
        stage('build') {
            steps {
                sh "mvn clean install"
            }
        }
        stage('deploy') {
            steps {
                deploy adapters: [tomcat9(credentialsId: '7356f8ee-5789-4f2d-b53d-64d264795c77', path: '', url: 'http://192.168.79.131:8081/')], contextPath: 'gvr6999', war: '**/*.war'
            }
        }
        stage('Sir') {
            steps {
                echo 'Thank you  Prasad sir'
            }
        }

    }
}
